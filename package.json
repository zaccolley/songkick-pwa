{
  "name": "songkick.pink",
  "version": "5.5.0",
  "description": "experimental songkick web app",
  "scripts": {
    "test": "npm run -s lint",
    "lint": "eslint ./src",
    "fix": "eslint --fix ./src",
    "clean": "rm -rf ./build",
    "prebuild": "npm run clean && mkdir -p build && ncp src/assets build/assets",
    "build": "NODE_ENV=production webpack -p --progress",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "dev": "node server.js",
    "deploy": "git pull && npm install && npm run build && pm2 restart pink"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.16.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "serviceworker-webpack-plugin": "^0.1.6",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-hot-middleware": "^2.16.1"
  },
  "dependencies": {
    "express": "^4.14.1",
    "get-image-colors": "^1.8.0",
    "localforage": "^1.4.3",
    "lowdb": "^0.15.5",
    "node-fetch": "^1.6.3",
    "postcss-will-change": "^1.1.0",
    "preact": "^7.2.0",
    "preact-router": "^2.4.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "unfetch": "^1.0.1",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack-dev-middleware": "^1.10.0"
  }
}
