{
  "name": "songkick.pink",
  "version": "6.0.2",
  "description": "progressive web app for songkick",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaccolley/songkick.pink.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "npm run -s lint",
    "lint": "eslint ./src && eslint ./server && eslint server.js",
    "lint:fix": "eslint --fix ./src && eslint --fix ./server && eslint --fix server.js",
    "clean": "rm -rf ./build",
    "prebuild": "npm run clean && mkdir -p build && ncp src/assets build/assets",
    "build": "NODE_ENV=production webpack --progress",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "dev": "node server.js",
    "deploy": "git pull && npm install && npm run build && pm2 restart pink",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.16.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.18.2",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "get-image-colors": "^1.8.1",
    "html-webpack-plugin": "^2.30.1",
    "localforage": "^1.4.3",
    "lowdb": "^1.0.0",
    "ncp": "^2.0.0",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "postcss-will-change": "^2.0.0",
    "preact": "^8.2.5",
    "preact-router": "^2.4.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "serviceworker-webpack-plugin": "^0.2.3",
    "style-loader": "^0.19.0",
    "unfetch": "^3.0.0",
    "url-loader": "^0.6.0",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "snyk": "^1.114.0"
  },
  "snyk": true
}
