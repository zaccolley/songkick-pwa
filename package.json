{
  "name": "songkick.pink",
  "version": "5.0.7",
  "description": "experimental songkick web app",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "npm run clean && mkdir -p build && ncp src/assets build/assets",
    "test": "npm run -s lint",
    "lint": "eslint {src,test}",
    "clean": "rm -rf ./build",
    "serve": "NODE_ENV=production node server.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.0.0",
    "ncp": "^2.0.0",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "serviceworker-webpack-plugin": "^0.1.6",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "superstatic": "^4.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-hot-middleware": "^2.16.1",
    "whatwg-fetch": "^2.0.2"
  },
  "dependencies": {
    "express": "^4.14.1",
    "geomicons-open": "^3.0.0-beta.1",
    "localforage": "^1.4.3",
    "preact": "^7.1.0",
    "preact-router": "^2.0.0",
    "web-push": "^3.2.1"
  }
}
